{
  "name": "vuejs-starter",
  "description": "vuejs starter kit providing webpack, vue-loader, babel and testing utilities",
  "version": "1.0.0",
  "author": "Hueseyin Yildiz <huess@juicyarts.de>",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "pack": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "jslint": "./node_modules/.bin/standard 'src/js/**/*.js' 'test/**/(!mocks)/*.js'",
    "sasslint": "./node_modules/.bin/sass-lint --config ./sass-lint.yml 'src/style/**/custom.scss' -v -q",
    "lint": "npm run sasslint && npm run jslint",
    "test:e2e": "gulp e2e",
    "test:unit": "./node_modules/.bin/karma start karma.conf.js",
    "test:unit:watch": "./node_modules/.bin/karma start karma.conf.js --singleRun=false",
    "test": "npm run test:unit && npm run pack && npm run test:e2e",
    "docs": "rm docs/jsdoc/api.md && ./node_modules/.bin/jsdoc2md src/**/*.js >> docs/jsdoc/api.md",
    "build": "npm run lint && npm test && npm run docs"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.5",
    "finalhandler": "^0.5.1",
    "http": "0.0.0",
    "serve-static": "^1.11.1",
    "vee-validate": "^2.0.0-beta.17",
    "vue": "^2.1.0",
    "vue-datetime-picker": "^0.2.1",
    "vue-paginate": "^3.1.0",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "bootstrap-loader": "^1.1.0",
    "chromedriver": "^2.25.3",
    "cross-env": "^3.0.0",
    "css-loader": "^0.26.1",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-serve": "^1.4.0",
    "jasmine-core": "^2.5.2",
    "jquery": "^3.1.1",
    "jsdoc-to-markdown": "^2.0.1",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-matchers": "^3.0.1",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "moment": "^2.17.1",
    "nightwatch": "^0.9.9",
    "node-sass": "^4.0.0",
    "phantomjs": "^2.1.7",
    "pug": "^2.0.0-beta6",
    "sass-lint": "^1.10.2",
    "sass-loader": "^4.0.2",
    "selenium-server-standalone-jar": "^3.0.1",
    "standard": "^8.6.0",
    "tether": "^1.4.0",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "standard": {
    "env": {
      "browser": true,
      "es6": true,
      "jasmine": true,
      "mocha": true,
      "node": true
    },
    "globals": [
      "jQuery",
      "$",
      "Moment",
      "moment"
    ]
  }
}
